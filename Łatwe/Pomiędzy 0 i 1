#include <iostream>
#include <vector>
#include <string>
#include <algorithm>

using namespace std;

int main()
{
    string text;
    int begin = 0;
    int end = 0;

    cin >> text;
    
    for (int i = 0; i < text.size(); i++) {
        if (text[i] == '0') {
            begin = i;
            break;
        }
    }

    for (int i = text.size() - 1; i > 0; i--) {
        if (text[i] == '1') {
            end = i;
            break;
        }
    }
    if ((end - begin - 1) == 0) cout << "PUSTY";
    else cout << text.substr(begin + 1, end - begin - 1) << endl;

    return 0;
}
