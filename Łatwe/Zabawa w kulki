#include <iostream>
#include <vector>
#include <string>
#include <algorithm>

using namespace std;

struct Winner {
    string name;
    int acceptedShoots;
};

int main()
{
    int x, y, radius, numberShoots, numberParticipants;
    string name;
    int shootX, shootY, hitCount, distance;
    vector <Winner> result;

    cin >> x >> y >> radius >> numberShoots >> numberParticipants;
    
    while (numberParticipants > 0) {
        cin >> name;
        hitCount = 0;

        for (int i = 0; i < numberShoots; i++) {
            cin >> shootX >> shootY;
            distance = (shootX - x) * (shootX - x) + (shootY - y) * (shootY - y);

            if (distance <= radius) hitCount++;
        }

        Winner person;
        person.name = name;
        person.acceptedShoots = hitCount;

        if (result.size() > 0) {
            if (hitCount > result[0].acceptedShoots){
                result[0].name = name;
                result[0].acceptedShoots = hitCount;
            }
        }
        else if (hitCount > 0) result.push_back(person);
            
        numberParticipants--;
    }

    if (result.size() > 0)
    cout << result[0].name << " " << result[0].acceptedShoots << endl;

    return 0;
}
